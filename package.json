{
  "$schema": "https://www.schemastore.org/package",
  "name": "vuebro",
  "version": "3.49.0",
  "description": "VueBro is an open-source browser-based Vue.js web editor that allows creating static websites with Vue SFCs without Node.js setup. Features runtime SFC compilation, Monaco editor, WYSIWYG mode, Tailwind CSS, and multiple storage options.",
  "keywords": [
    "vuejs",
    "web-editor",
    "browser-based",
    "open-source",
    "sfc",
    "single-file-components",
    "runtime-compilation",
    "monaco-editor",
    "wysiwyg",
    "tailwind-css",
    "unocss",
    "quasar",
    "typescript",
    "vite",
    "static-websites",
    "no-setup",
    "development-environment",
    "electron",
    "cross-platform",
    "i18n"
  ],
  "license": "AGPL-3.0-or-later",
  "author": "Jerry Bruwes <jbruwes@gmail.com> (https://jbruwes.github.io)",
  "type": "module",
  "scripts": {
    "build": "quasar build",
    "build:electron": "quasar build -m electron",
    "dev": "quasar dev",
    "dev:electron": "quasar dev -m electron",
    "postinstall": "quasar prepare",
    "lint": "eslint ."
  },
  "prettier": "@vuebro/configs/prettierrc",
  "dependencies": {
    "@ai-sdk/mistral": "^2.0.24",
    "@aws-sdk/client-s3": "^3.932.0",
    "@electron/remote": "^2.1.3",
    "@iconify/vue": "^5.0.0",
    "@quasar/app-vite": "^2.4.0",
    "@quasar/quasar-ui-qiconpicker": "^2.0.7",
    "@shikijs/monaco": "^3.15.0",
    "@smithy/fetch-http-handler": "^5.3.6",
    "@unocss/runtime": "^66.5.6",
    "@vuebro/fsa": "1.0.10",
    "@vuebro/monaco-sfc": "^1.0.37",
    "@vuebro/shared": "^1.2.9",
    "@vueuse/core": "^14.0.0",
    "ai": "^5.0.93",
    "consola": "^3.4.2",
    "crypto-es": "^3.1.2",
    "dompurify": "^3.3.0",
    "globals": "^16.5.0",
    "jsonfeed-to-atom": "^1.2.5",
    "jsonfeed-to-rss": "^3.0.8",
    "marked": "^17.0.0",
    "marked-shiki": "^1.2.1",
    "mime": "^4.1.0",
    "monaco-editor": "^0.52.2",
    "monaco-tailwind": "^0.1.0-alpha.2",
    "monacopilot": "^1.2.7",
    "ofetch": "^1.5.1",
    "path-browserify": "^1.0.1",
    "pinia": "^3.0.4",
    "quasar": "^2.18.6",
    "shiki": "^3.15.0",
    "to-xml": "^0.1.11",
    "vue": "^3.5.24",
    "vue-i18n": "^11.1.12",
    "vue-router": "^4.6.3",
    "vue-sfc-descriptor-to-string": "^3.0.1"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^11.0.1",
    "@quasar/extras": "^1.17.0",
    "@quasar/quasar-app-extension-qiconpicker": "^2.0.7",
    "@types/crypto-js": "^4.2.2",
    "@types/path-browserify": "^1.0.3",
    "@types/wicg-file-system-access": "^2023.10.7",
    "@vuebro/configs": "^1.2.7",
    "@vuebro/runtime": "^1.2.19",
    "autoprefixer": "^10.4.22",
    "electron": "^39.2.1",
    "electron-builder": "^26.0.12",
    "eslint": "^9.39.1",
    "unocss": "^66.5.6",
    "vite": "^7.2.2",
    "vite-plugin-checker": "^0.11.0",
    "vite-plugin-static-copy": "^3.1.4"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "overrideBrowserslist": [
          "last 4 Chrome versions",
          "last 4 Firefox versions",
          "last 4 Edge versions",
          "last 4 Safari versions",
          "last 4 Android versions",
          "last 4 ChromeAndroid versions",
          "last 4 FirefoxAndroid versions",
          "last 4 iOS versions"
        ]
      }
    }
  },
  "productName": "VueBro"
}
