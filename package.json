    "@smithy/fetch-http-handler": "^5.3.5",
    "@unocss/runtime": "^66.5.5",
    "@vuebro/fsa": "1.0.2",
    "@vuebro/monaco-sfc": "^1.0.32",
    "@vuebro/shared": "^1.1.53",
    "@vueuse/core": "^14.0.0",
    "ai": "^5.0.89",
    "consola": "^3.4.2",
    "crypto-js": "^4.2.0",
    "dompurify": "^3.3.0",
    "globals": "^16.5.0",
    "jsonfeed-to-atom": "^1.2.5",
    "jsonfeed-to-rss": "^3.0.8",
    "marked": "^17.0.0",
    "marked-shiki": "^1.2.1",
    "mime": "^4.1.0",
    "monaco-editor": "^0.52.2",
    "monaco-tailwind": "^0.1.0-alpha.2",
    "monacopilot": "^1.2.7",
    "ofetch": "^1.5.1",
    "path-browserify": "^1.0.1",
    "quasar": "^2.18.5",
    "shiki": "^3.15.0",
    "to-xml": "^0.1.11",
    "vue": "^3.5.24",
    "vue-i18n": "^11.1.12",
    "vue-router": "^4.6.3",
    "vue-sfc-descriptor-to-string": "^3.0.1"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^11.0.1",
    "@quasar/extras": "^1.17.0",
    "@quasar/quasar-app-extension-qiconpicker": "^2.0.7",
    "@types/crypto-js": "^4.2.2",
    "@types/path-browserify": "^1.0.3",
    "@types/wicg-file-system-access": "^2023.10.7",
    "@vuebro/configs": "^1.1.63",
    "@vuebro/runtime": "^1.2.9",
    "autoprefixer": "^10.4.21",
    "electron": "^39.1.1",
    "electron-builder": "^26.0.12",
    "eslint": "^9.39.1",
    "unocss": "^66.5.5",
    "vite": "^7.2.2",
    "vite-plugin-checker": "^0.11.0",
    "vite-plugin-static-copy": "^3.1.4"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "overrideBrowserslist": [
          "last 4 Chrome versions",
          "last 4 Firefox versions",
          "last 4 Edge versions",
          "last 4 Safari versions",
          "last 4 Android versions",
          "last 4 ChromeAndroid versions",
          "last 4 FirefoxAndroid versions",
          "last 4 iOS versions"
        ]
      }
    }
